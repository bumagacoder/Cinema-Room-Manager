package stage1 // first stage of the project, Arrangement.

fun main() {
    println("Cinema:")
    println("  1 2 3 4 5 6 7 8")
    for (i in 1..7) {
        var row = "$i "
        for (j in 1..8) {
            row += "S "
        }
        println(row)
    }
}
